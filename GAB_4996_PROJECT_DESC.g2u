F.Intrinsic.Control.If(V.Screen.F_PRJ!txtID.Text,=,"")
	F.Intrinsic.UI.Msgbox("Click New or Select a Project ID","Invalid Project ID number")
F.Intrinsic.Control.Else
	V.Local.iID.Set(V.Screen.F_PRJ!txtID.Text)
	V.Local.sSql.Set("Select * From GAB_PRJ_DESC ")
	F.Intrinsic.String.Concat(V.Local.sSql,"WHERE ID_PRJ=",V.Local.iID,V.Local.sSql)
	F.ODBC.Connection!Conx.OpenRecordsetRW("rstSavePrj",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.Conx!rstSavePrj.EOF,=,"True")
		'Check if project number is already used
		V.Local.sSql.Set("Select PRJ_START From GAB_PRJ_START WHERE TYPE='S'")
		F.ODBC.Connection!conx.ExecuteAndReturn(V.Local.sSql,V.Local.iID)
		F.Intrinsic.Math.Add(V.Local.iID,0,V.Local.iID)
		F.Intrinsic.Math.Add(V.Local.iID,1,V.Global.iNextPrjID)
		F.Intrinsic.String.Build("update GAB_prj_start set prj_start = {0} where type = 'S'",V.Global.iNextPrjID,V.Local.sSql)
		F.ODBC.Connection!conx.Execute(V.Local.sSql)
		
		F.ODBC.Conx!rstSavePrj.AddNew
		F.ODBC.Conx!rstSavePrj.Set!ID_PRJ(V.Local.iID)
		F.ODBC.Conx!rstSavePrj.Set!DATE_CREATED(V.Ambient.Date)
		F.ODBC.Conx!rstSavePrj.Set!USER_CREATED(V.Caller.User)
'		F.Intrinsic.Math.Add(V.Global.iNextPrjID,1,V.Global.iNextPrjID)
''Save next Project ID Number
'		V.Local.sSql.Set("Select PRJ_START,TYPE From GAB_PRJ_START ")
'		F.Intrinsic.String.Concat(V.Local.sSql," WHERE TYPE='S'",V.Local.sSql)
'		F.ODBC.Connection!Conx.OpenRecordsetRW("rstSavePrjID",V.Local.sSql)
'		F.Intrinsic.Control.If(V.ODBC.conx!rstSavePrjID.EOF,=,"False")
'			F.ODBC.conx!rstSavePrjID.Set!PRJ_START(V.Global.iNextPrjID)
'			F.ODBC.conx!rstSavePrjID.Update
'		F.Intrinsic.Control.EndIf
'		F.ODBC.Conx!rstSavePrjID.Close
		
	F.Intrinsic.Control.EndIf
	F.ODBC.Conx!rstSavePrj.Set!DESCR_PRJ(V.Screen.F_PRJ!txtDesc.Text)
	F.Intrinsic.Control.If(V.Screen.F_PRJ!optActive.Value,=,"True")
		F.ODBC.Conx!rstSavePrj.Set!STATUS("A")
		F.ODBC.Conx!rstSavePrj.Set!DATE_CLOSED("1900-01-01")
		F.ODBC.Conx!rstSavePrj.Set!USER_CLOSED("")
	F.Intrinsic.Control.ElseIf(V.Screen.F_PRJ!optHold.Value,=,"True")
		F.ODBC.Conx!rstSavePrj.Set!STATUS("H")
	F.Intrinsic.Control.ElseIf(V.Screen.F_PRJ!optClosed.Value,=,"True")
		F.ODBC.Conx!rstSavePrj.Set!STATUS("C")
		F.ODBC.Conx!rstSavePrj.Set!DATE_CLOSED(V.Ambient.Date)
		F.ODBC.Conx!rstSavePrj.Set!USER_CLOSED(V.Caller.User)
	F.Intrinsic.Control.EndIf
	F.ODBC.Conx!rstSavePrj.Set!CUSTOMER(V.Screen.F_PRJ!txtCust.Text)
	F.ODBC.Conx!rstSavePrj.Set!PM(V.Global.sPMcode)
	F.ODBC.Conx!rstSavePrj.Set!SP(V.Global.sSPCode)
	F.ODBC.Conx!rstSavePrj.Set!DATE_START(V.Screen.F_PRJ!dtpStart.Value)
	F.ODBC.Conx!rstSavePrj.Set!DATE_DUE(V.Screen.F_PRJ!dtpDue.Value)
	F.ODBC.Conx!rstSavePrj.Set!UF1(V.Screen.F_PRJ!txtUF1.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF2(V.Screen.F_PRJ!txtUF2.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF3(V.Screen.F_PRJ!txtUF3.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF4(V.Screen.F_PRJ!txtUF4.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF5(V.Screen.F_PRJ!txtUF5.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF6(V.Screen.F_PRJ!txtUF6.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF7(V.Screen.F_PRJ!txtUF7.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF8(V.Screen.F_PRJ!txtUF8.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF9(V.Screen.F_PRJ!txtUF9.Text)
	F.ODBC.Conx!rstSavePrj.Set!UF10(V.Screen.F_PRJ!txtUF10.Text)

	F.ODBC.Conx!rstSavePrj.Update
	F.ODBC.Conx!rstSavePrj.Close
	V.Local.sSql2.Set("Select * From GAB_PRJ_MAIN ")
	F.Intrinsic.String.Concat(V.Local.sSql2,"WHERE ID_PRJ='",V.Screen.F_PRJ!txtID.Text,"'",V.Local.sSql2)
	F.ODBC.Connection!Conx.OpenRecordsetRW("rstSaveMPrj",V.Local.sSql2)
	F.Intrinsic.Control.If(V.ODBC.conx!rstSaveMPrj.EOF,=,"True")
		F.ODBC.conx!rstSaveMPrj.AddNew
		F.ODBC.conx!rstSaveMPrj.Set!ID_PRJ(V.Screen.F_PRJ!txtID.Text)
	F.Intrinsic.Control.EndIf
	F.ODBC.conx!rstSaveMPrj.Set!DESCR_PRJ(V.Screen.F_PRJ!txtDesc.Text)
	F.Intrinsic.Control.If(V.Screen.F_PRJ!optActive.Value,=,"True")
		F.ODBC.Conx!rstSaveMPrj.Set!STATUS("A")
	F.Intrinsic.Control.ElseIf(V.Screen.F_PRJ!optHold.Value,=,"True")
		F.ODBC.Conx!rstSaveMPrj.Set!STATUS("H")
	F.Intrinsic.Control.ElseIf(V.Screen.F_PRJ!optClosed.Value,=,"True")
		F.ODBC.Conx!rstSaveMPrj.Set!STATUS("C")
	F.Intrinsic.Control.EndIf
