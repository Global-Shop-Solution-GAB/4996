program.sub.cmdid_click.start
F.Intrinsic.Control.SetErrorHandler("cmdid_click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)

V.Local.sTitles.Declare(String)
V.Local.iWidths.Declare(Long)
V.Local.sRet.Declare(String)
V.Local.sSql.Declare(String)
F.Intrinsic.String.Split("PRJ ID*!*Description*!*Status","*!*",V.Local.sTitles)
F.Intrinsic.String.Split("1300*!*2300*!*1300","*!*",V.Local.iWidths)

F.Intrinsic.UI.SetBrowserHotTypeAhead(True)
F.Intrinsic.UI.MiniBrowser("Select a Project",conx,"Select ID_PRJ, DESCR_PRJ, STATUS FROM GAB_PRJ_DESC ORDER BY ID_PRJ DESC",V.Local.sTitles,V.Local.iWidths,V.Local.sRet)

F.Intrinsic.Control.If(V.Local.sRet,"=","***CANCEL***")
	F.Intrinsic.UI.Msgbox("No Project ID was selected")
F.Intrinsic.Control.Else
	F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
	Gui.F_PRJ.txtID.Text(V.Local.sRet(0))
	Gui.F_PRJ.txtDesc.Text(V.Local.sRet(1).Trim)
	F.Intrinsic.Control.If(V.Local.sRet(2),=,"A")
		Gui.F_PRJ.optActive.Value("True")
	F.Intrinsic.Control.ElseIf(V.Local.sRet(2),=,"H")
		Gui.F_PRJ.optHold.Value("True")
	F.Intrinsic.Control.ElseIf(V.Local.sRet(2),=,"C")
		Gui.F_PRJ.optClosed.Value("True")
	F.Intrinsic.Control.EndIf

	F.Intrinsic.String.Concat("Select * from GAB_PRJ_DESC Where ID_PRJ='",V.Local.sRet(0),"'",V.Local.sSql)

	F.ODBC.connection!conx.OpenRecordsetRO("rstLoad",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.conx!rstLoad.EOF,=,False)
		Gui.F_PRJ.txtCust.Text(V.ODBC.conx!rstLoad.FieldValTrim!CUSTOMER)
		F.Intrinsic.Control.If(V.ODBC.conx!rstLoad.FieldValTrim!CUSTOMER,<>,"")
			F.Intrinsic.String.Concat("SELECT NAME_CUSTOMER from V_CUSTOMER_MASTER WHERE CUSTOMER='",V.ODBC.conx!rstLoad.FieldValTrim!CUSTOMER,"'",V.Local.sSql)
			F.ODBC.connection!conx.OpenRecordsetRO("rstLoadCust",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.conx!rstLoadCust.EOF,=,False)
				Gui.F_PRJ.txtCustName.Text(V.ODBC.conx!rstLoadCust.FieldValTrim!NAME_CUSTOMER)
			F.Intrinsic.Control.EndIf
			F.ODBC.conx!rstLoadCust.Close
		F.Intrinsic.Control.Else
			Gui.F_PRJ.txtCustName.Text("")
		F.Intrinsic.Control.EndIf
		Gui.F_PRJ.dtpStart.Value(V.ODBC.conx!rstLoad.FieldVal!DATE_START)
		Gui.F_PRJ.dtpDue.Value(V.ODBC.conx!rstLoad.FieldVal!DATE_DUE)
		V.Global.sPMcode.Set(V.ODBC.conx!rstLoad.FieldValTrim!PM)
		F.Intrinsic.Control.If(V.ODBC.conx!rstLoad.FieldValTrim!PM,<>,"")
			F.Intrinsic.String.Concat("SELECT NAME from GAB_PRJ_PM WHERE IDM='",V.ODBC.conx!rstLoad.FieldValTrim!PM,"'",V.Local.sSql)
			F.ODBC.connection!conx.OpenRecordsetRO("rstLoadPM",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.conx!rstLoadPM.EOF,=,False)
				Gui.F_PRJ.txtPM.Text(V.ODBC.conx!rstLoadPM.FieldValTrim!NAME)
			F.Intrinsic.Control.EndIf
			F.ODBC.conx!rstLoadPM.Close
		F.Intrinsic.Control.Else
			Gui.F_PRJ.txtPM.Text("")
		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.If(V.ODBC.conx!rstLoad.FieldValTrim!SP,<>,"")
			F.Intrinsic.String.Concat("SELECT SALESPERSON from V_SALESPEOPLE WHERE SALESPERSON_CODE='",V.ODBC.conx!rstLoad.FieldValTrim!SP,"'",V.Local.sSql)
			F.ODBC.connection!conx.OpenRecordsetRO("rstLoadSP",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.conx!rstLoadSP.EOF,=,False)
				Gui.F_PRJ.txtSP.Text(V.ODBC.conx!rstLoadSP.FieldValTrim!SALESPERSON)
			F.Intrinsic.Control.EndIf
			F.ODBC.conx!rstLoadSP.Close
		F.Intrinsic.Control.Else
			Gui.F_PRJ.txtSP.Text("")
		F.Intrinsic.Control.EndIf
		V.Global.sSPCode.Set(V.ODBC.conx!rstLoad.FieldValTrim!SP)
		Gui.F_PRJ.txtUF1.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF1)
		Gui.F_PRJ.txtUF2.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF2)
		Gui.F_PRJ.txtUF3.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF3)
		Gui.F_PRJ.txtUF4.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF4)
		Gui.F_PRJ.txtUF5.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF5)
		Gui.F_PRJ.txtUF6.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF6)
		Gui.F_PRJ.txtUF7.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF7)
		Gui.F_PRJ.txtUF8.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF8)
		Gui.F_PRJ.txtUF9.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF8)
		Gui.F_PRJ.txtUF10.Text(V.ODBC.conx!rstLoad.FieldValTrim!UF10)

	F.Intrinsic.Control.EndIf
	F.ODBC.conx!rstLoad.Close

F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdid_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GAB_PRJ_DESC.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.Debug.SetLA(V.Local.sError)
Function.Intrinsic.Control.EndIf

